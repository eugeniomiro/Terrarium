<?xml version="1.0"?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <clear />
        <add name="debugLog"
             type="System.Diagnostics.TextWriterTraceListener"
             initializeData="debug.log" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>
